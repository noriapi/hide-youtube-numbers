  },
  "license": "MIT",
  "author": {
    "name": "noriapi"
  },
  "type": "module",
  "scripts": {
    "build": "wxt build",
    "build:firefox": "wxt build -b firefox",
    "compile": "tsc --noEmit",
    "dev": "wxt",
    "dev:firefox": "wxt -b firefox",
    "e2e": "playwright test",
